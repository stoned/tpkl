amends "pkl:test"

import "tpkl:tpkl"

examples {
  ["default"] {
    new tpkl.Tasks {
      ["default"] {
        cmds {
          new {
            cmd { "echo default" }
          }
        }
      }
    }
  }

  ["cmd"] {
    new tpkl.Tasks {
      ["cmd"] {
        cmds {
          new {
            embeddedShell = false
            cmd { "echo"; "cmd" }
          }
        }
      }
    }
  }
}
